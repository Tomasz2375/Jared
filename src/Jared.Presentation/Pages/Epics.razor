@page "/epics"
@inject IMediator Mediator

@if (Model is null)
{
    return;
}

<DataGrid
    Title="Epics"
    TItem="EpicListDto"
    Items="Model.Epics"
    ItemsDefinition="new EpicListDefinition().GetColumns()"
    Pagination="Model.Pagination"
    SwitchPagination="true"
    Query="Query"
    SendPageQuery="sendPageQuery" />
